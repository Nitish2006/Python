   "source": [
    "from concurrent.futures import ThreadPoolExecutor\n",
    "import time\n",
    "\n",
    "def prin(n):\n",
    "    time.sleep(2)\n",
    "    return f\"Number : {n}\"\n",
    "\n",
    "n = [1, 2, 3, 4, 5]\n",
    "\n",
    "with ThreadPoolExecutor(max_workers=3) as executor:\n",
    "    res = executor.map(prin, n)\n",
    "\n",
    "for r in res:\n",
    "    print(r)"
   ]
  }
